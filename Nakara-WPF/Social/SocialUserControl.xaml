<UserControl
    x:Class="Nakara_WPF.Social.SocialUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Nakara_WPF.Social"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:Background="Gray"
    d:DesignHeight="100"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Nakara.Controls;component/Themes/DefaultStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <UniformGrid Grid.Column="3" Columns="3">
        <Image Style="{StaticResource Email}" />
        <Image Style="{StaticResource Contact}" />
        <Image Style="{StaticResource Setting}" />
    </UniformGrid>
</UserControl>
