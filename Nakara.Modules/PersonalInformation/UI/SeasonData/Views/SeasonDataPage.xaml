<bases:UserControlBase
    x:Class="Nakara.Modules.PersonalInformation.UI.SeasonData.Views.SeasonDataPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:bases="clr-namespace:Nakara.Framework.Core.Bases;assembly=Nakara.Framework"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Nakara.Modules.PersonalInformation.UI.SeasonData.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:Background="Gray"
    d:DesignHeight="720"
    d:DesignWidth="1080"
    mc:Ignorable="d">
    <Grid Margin="30">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.15*" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid Grid.Row="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.5*" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Grid
                Grid.Column="0"
                Margin="0,0,10,0"
                Background="#3A000000" />
            <Grid Grid.Column="1">
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.5*" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <UniformGrid
                    Margin="0,0,0,10"
                    Background="#3A000000"
                    Columns="6" />
                <UniformGrid Grid.Row="1" Columns="3">
                    <UniformGrid
                        Margin="0,0,5,0"
                        Background="#3A000000"
                        Rows="3">
                        <UniformGrid Columns="2" />
                    </UniformGrid>
                    <UniformGrid
                        Margin="5,0"
                        Background="#3A000000"
                        Rows="3">
                        <UniformGrid Columns="2" />
                    </UniformGrid>
                    <UniformGrid
                        Margin="5,0,0,0"
                        Background="#3A000000"
                        Rows="3">
                        <UniformGrid Columns="2" />
                    </UniformGrid>
                </UniformGrid>
            </Grid>
        </Grid>
    </Grid>
</bases:UserControlBase>